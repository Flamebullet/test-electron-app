{
	"name": "youtube-downloader",
	"version": "1.0.17",
	"description": "Application to download YouTube Videos",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "env-cmd electron-forge publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Flamebullet/youtube-downloader.git"
	},
	"author": "Flame",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Flamebullet/youtube-downloader.git"
	},
	"homepage": "https://github.com/Flamebullet/youtube-downloader#readme",
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.67",
		"@electron-forge/maker-deb": "^6.0.0-beta.67",
		"@electron-forge/maker-rpm": "^6.0.0-beta.67",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.67",
		"@electron-forge/maker-zip": "^6.0.0-beta.67",
		"@electron-forge/publisher-github": "^6.0.0-beta.67",
		"electron": "^21.2.0"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"name": "YouTube-Downloader",
				"osxSign": {
					"identity": "Developer ID Application: YouTube Downloader"
				},
				"icon": "./content/youtube-icon.ico"
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "YouTube-Downloader",
						"exe": "YouTube-Downloader.exe"
					}
				}
			],
			"publishers": [
				{
					"name": "@electron-forge/publisher-github",
					"platforms": [
						"win32"
					],
					"config": {
						"repository": {
							"owner": "Flamebullet",
							"name": "youtube-downloader"
						},
						"draft": true
					}
				}
			]
		}
	},
	"dependencies": {
		"@electron/remote": "^2.0.8",
		"check-internet-connected": "^2.0.6",
		"child_process": "^1.0.2",
		"electron-simple-updater": "^2.0.11",
		"electron-squirrel-startup": "^1.0.0",
		"env-cmd": "^10.1.0",
		"ffmpeg-static": "^5.1.0",
		"fs": "^0.0.1-security",
		"spotify-to-yt": "^1.1.1",
		"sweetalert": "^2.1.2",
		"yt-search": "^2.10.3",
		"ytdl-core": "^4.11.2"
	}
}
