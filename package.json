{
	"name": "test-electron-app",
	"version": "1.0.1",
	"description": "Electron app for testing purposes",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "env-cmd electron-forge publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Flamebullet/youtube-downloader.git"
	},
	"author": "Flame",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Flamebullet/youtube-downloader.git"
	},
	"homepage": "https://github.com/Flamebullet/youtube-downloader#readme",
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.61",
		"@electron-forge/maker-deb": "^6.0.0-beta.61",
		"@electron-forge/maker-rpm": "^6.0.0-beta.61",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.61",
		"@electron-forge/maker-zip": "^6.0.0-beta.61",
		"@electron-forge/publisher-github": "^6.0.0-beta.61",
		"electron": "^16.0.5"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"name": "testapp",
				"osxSign": {
					"identity": "Developer ID Application: Test"
				}
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "testapp",
						"exe": "testapp.exe"
					}
				}
			],
			"publishers": [
				{
					"name": "@electron-forge/publisher-github",
					"platforms": [
						"win32"
					],
					"config": {
						"repository": {
							"owner": "Flamebullet",
							"name": "youtube-downloader"
						},
						"draft": true
					}
				}
			]
		}
	},
	"dependencies": {
		"electron-squirrel-startup": "^1.0.0",
		"env-cmd": "^10.1.0"
	}
}
